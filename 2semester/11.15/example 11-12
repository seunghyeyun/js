<!--(예제 11-12) 애니메이션 효과-->
<!DOCTYPE html>
<html>
<head>
    <title>Animate Basic</title>
    <script src="https://code.jquery.com/jquery-3.4.1.js"></script>
    <script>
        $(document).ready(function(){
            $('#box').css({
                width:100,
                height:100,
                background:'red'
            }).animate({
                width:300,
                opacity:0.5,
            },500);
        });
    </script>
</head>
<body>
    <dic id="box"></dic>
</body>
</html>
